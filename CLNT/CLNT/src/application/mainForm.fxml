<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="application.mainFormController">
   <bottom>
      <AnchorPane prefHeight="424.0" prefWidth="521.0" BorderPane.alignment="CENTER">
         <children>
            <Text fill="#ab0fab" layoutX="51.0" layoutY="86.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome To Ort Braude Library">
               <font>
                  <Font name="System Bold Italic" size="29.0" />
               </font>
            </Text>
            <Button fx:id="signIn" layoutX="213.0" layoutY="154.0" mnemonicParsing="false" onAction="#signInFunc" prefHeight="31.0" prefWidth="95.0" text="Sign-In" />
            <Button fx:id="signup" layoutX="271.0" layoutY="196.0" mnemonicParsing="false" onAction="#signupFunc" prefHeight="32.0" prefWidth="106.0" text="Sign-Up" />
            <Button fx:id="search" layoutX="216.0" layoutY="244.0" mnemonicParsing="false" onAction="#searchFunc" prefHeight="31.0" prefWidth="106.0" text="Search Book" />
            <Button fx:id="exit" layoutX="14.0" layoutY="387.0" mnemonicParsing="false" onAction="#exitFunc" text="Exit" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
