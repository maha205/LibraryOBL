<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <AnchorPane prefHeight="333.0" prefWidth="428.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="IDinput" alignment="CENTER" layoutX="34.0" layoutY="23.0" prefHeight="36.0" prefWidth="187.0" promptText="Student ID" />
            <Button fx:id="SBMT" layoutX="238.0" layoutY="19.0" onAction="#getid" prefHeight="31.0" prefWidth="81.0" text="search" textFill="#3600fb">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font></Button>
            <Text fx:id="FirstName" fill="#3600fb" layoutX="31.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0" text="First Name" visible="false">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Text>
            <Text fx:id="Operation" fill="#3600fb" layoutX="36.0" layoutY="225.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Operation" visible="false">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Text>
            <Text fx:id="IDtxt" fill="#3600fb" layoutX="34.0" layoutY="142.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ID" visible="false">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Text>
            <Text fx:id="Status" fill="#3600fb" layoutX="36.0" layoutY="183.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Status" visible="false">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Text>
            <Button fx:id="UPDT" layoutX="281.0" layoutY="259.0" onAction="#Update" text="Update" textFill="#3600fb" visible="false">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Button>
            <Text fx:id="Errormsg" fill="RED" layoutX="146.0" layoutY="112.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Not Found ! " visible="false">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font></Text>
            <ComboBox fx:id="cmboChoose" layoutX="94.0" layoutY="259.0" prefHeight="30.0" prefWidth="171.0" promptText="Choose Satatus" visible="false" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
