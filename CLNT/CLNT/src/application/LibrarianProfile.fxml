<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="411.0" prefWidth="643.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LibrarianProfileController">
   <top>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Text layoutX="14.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Hello, Librarian!" />
            <Text fill="#ae1d1d" layoutX="178.0" layoutY="86.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Librarian Profile">
               <font>
                  <Font name="System Bold Italic" size="37.0" />
               </font>
            </Text>
            <Text layoutX="150.0" layoutY="143.0" strokeType="OUTSIDE" strokeWidth="0.0" />
            <MenuButton fx:id="Options" layoutX="237.0" layoutY="111.0" mnemonicParsing="false" onAction="#OptionsFunc" text="Librarian options">
              <items>
                <MenuItem mnemonicParsing="false" text="student return book" />
                <MenuItem mnemonicParsing="false" text="edit student profile" />
                  <MenuItem mnemonicParsing="false" text="open new subscription to student" />
                  <MenuItem mnemonicParsing="false" text="update student card's status " />
              </items>
            </MenuButton>
            <Button fx:id="editBtn" layoutX="537.0" layoutY="20.0" mnemonicParsing="false" onAction="#editFunc" text="Edit Profile" />
            <Button fx:id="logout" layoutX="14.0" layoutY="366.0" mnemonicParsing="false" onAction="#logoutFunc" text="Log-Out" />
         </children>
      </AnchorPane>
   </top>
</BorderPane>
