<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StudentProfileController">
   <top>
      <AnchorPane prefHeight="579.0" prefWidth="876.0" BorderPane.alignment="CENTER">
         <children>
            <Text layoutX="14.0" layoutY="20.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Hello, Student!" />
            <Text fill="#ae1d1d" layoutX="283.0" layoutY="112.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Subscriber Profile">
               <font>
                  <Font name="System Bold Italic" size="37.0" />
               </font>
            </Text>
            <Text fill="#ae1d1d" layoutX="283.0" layoutY="145.0" strokeType="OUTSIDE" strokeWidth="0.0">
               <font>
                  <Font name="System Bold Italic" size="17.0" />
               </font>
            </Text>
            <Button fx:id="edit" layoutX="781.0" layoutY="-1.0" mnemonicParsing="false" onAction="#editFunc" prefHeight="26.0" prefWidth="95.0" text="Edit Profile" />
            <Button fx:id="signUp" layoutX="26.0" layoutY="537.0" mnemonicParsing="false" onAction="#signUpFunc" text="Log-out" />
            <Button layoutX="674.0" layoutY="-1.0" mnemonicParsing="false" onAction="#searchBook" prefHeight="17.0" prefWidth="107.0" text="Search Book" />
            <Button layoutX="518.0" layoutY="-1.0" mnemonicParsing="false" onAction="#ExtendLoanDuration" prefHeight="23.0" prefWidth="165.0" text="Extend Loan Duration" />
            <Button layoutX="416.0" layoutY="-1.0" mnemonicParsing="false" onAction="#showStudentInfo" text="Student Info" />
